{
  "build": {
    "env": {
      "NODE_VERSION": "18"
    }
  },
  "functions": {
    "api/contact.py": {
      "runtime": "python@3.9"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/contact.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

